{
  "latex-workshop.latex.recipes": [
    {
      "name": "uplatex",
      "tools": ["uplatex", "uplatex", "dvipdfmx"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "uplatex",
      "command": "uplatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    },
    {
      "name": "dvipdfmx",
      "command": "dvipdfmx",
      "args": ["%DOCFILE%.dvi"]
    }
  ],
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.log",
    "*.dvi",
    "*.synctex.gz",
    "*.fls",
    "*.fdb_latexmk"
  ]
}
